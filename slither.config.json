{
  "filter_paths": [
    "lib/",
    "node_modules/",
    "test/",
    "script/"
  ],
  "exclude_dependencies": true,
  "exclude_optimization": false,
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "solc_remaps": [
    "@openzeppelin/contracts/=lib/openzeppelin-contracts/contracts/",
    "@chainlink/contracts/=lib/chainlink-brownie-contracts/contracts/",
    "forge-std/=lib/forge-std/src/"
  ],
  "detectors_to_exclude": [
    "pragma",
    "solc-version",
    "low-level-calls",
    "naming-convention"
  ],
  "detectors_to_include": [
    "reentrancy-eth",
    "reentrancy-no-eth",
    "reentrancy-unlimited-gas",
    "uninitialized-state",
    "uninitialized-storage",
    "arbitrary-send-eth",
    "controlled-delegatecall",
    "delegatecall-loop",
    "msg-value-loop",
    "reentrancy-benign",
    "reentrancy-events",
    "timestamp",
    "assembly",
    "assert-state-change",
    "boolean-equal",
    "deprecated-standards",
    "erc20-interface",
    "erc721-interface",
    "incorrect-equality",
    "locked-ether",
    "mapping-deletion",
    "shadowing-abstract",
    "shadowing-builtin",
    "shadowing-local",
    "shadowing-state",
    "calls-loop",
    "events-access",
    "events-maths",
    "incorrect-unary",
    "missing-zero-check",
    "reentrancy-readonly",
    "reused-constructor",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "uninitialized-local",
    "unused-return",
    "incorrect-modifier",
    "shadowing-modifier",
    "suicidal",
    "unprotected-upgrade",
    "domain-separator-collision",
    "enum-conversion",
    "erc20-indexed",
    "function-init-state",
    "incorrect-shift",
    "multiple-constructors",
    "name-reused",
    "protected-vars",
    "public-mappings-nested",
    "redundant-statements",
    "rtlo",
    "similar-names",
    "too-many-digits",
    "constable-states",
    "external-function",
    "immutable-states",
    "var-read-using-this"
  ],
  "compilation_unit_ignore": [],
  "disable_color": false,
  "generate_patches": false,
  "legacy_ast": false,
  "ignore_return_value": false,
  "zip": null,
  "zip_type": "lzma",
  "show_ignored_findings": false,
  "sarif": "results.sarif",
  "json": "results.json",
  "checklist": false,
  "markdown": false
}